unit uIRepositoryVeiculo;

interface

uses uVeiculos,uDtoVeiculo, system.Generics.Collections;


type

 IRepositoryVeiculo = interface

  procedure Cadastrar(veiculo : TVeiculo);
  procedure Alterar(veiculo : Tveiculo);
  procedure Excluir(codigo : integer);
  Function Consultar(dto: DtoVeiculo) :TObjectlist<TVeiculo>;

 end;


implementation

end.
